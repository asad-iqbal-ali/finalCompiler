.globl main
  .type main,@function
main:
  enter $384, $0
  pushl $.s0
  popl %edx
  pushl $128
  pushl %edx
  leal -128(%ebp), %eax
  pushl %eax
  call strncpy
  movb $0, 127(%eax)
  addl $12, %esp
  pushl $.s1
  popl %edx
  pushl $128
  pushl %edx
  leal -256(%ebp), %eax
  pushl %eax
  call strncpy
  movb $0, 127(%eax)
  addl $12, %esp
  pushl $.s2
  popl %edx
  pushl $128
  pushl %edx
  leal -384(%ebp), %eax
  pushl %eax
  call strncpy
  movb $0, 127(%eax)
  addl $12, %esp
  pushl $.s3
  call printf
  addl $4, %esp
  pushl $.s5
  pushl $.s4
  popl %eax
  popl %ecx
  pushl $128
  pushl %eax
  push $.stracc
  call strncpy
  movb $0, 127(%eax)
  addl $12, %esp
  pushl %ecx
  push $.stracc
  call strcat
  movb $0, 127(%eax)
  addl $8, %esp
  pushl $0
  popl %eax
  jmp .main_ret
.main_ret:
  leave
  ret
	
	.comm .strres,256,4
	.comm .stracc,512,4



.section	.rodata
	.s0:	.string	"hello"
	.s1:	.string	" world\n"
	.s2:	.string	"bye"
	.s3:	.string	"hello"
	.s4:	.string	"hello"
	.s5:	.string	"world\n"
	.s6:	.string	"world\n"
	.s7:	.string	"hello"
